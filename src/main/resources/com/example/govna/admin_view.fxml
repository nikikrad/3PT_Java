<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="400.0" prefWidth="600.0">
    <children>
        <AnchorPane layoutX="8.0" layoutY="8.0" nodeOrientation="LEFT_TO_RIGHT" prefHeight="389.0" prefWidth="588.0"
                    style="-fx-background-color: #4444;">
            <children>
                <Button fx:id="btnAddStation" layoutX="175.0" layoutY="156.0" mnemonicParsing="false"
                        text="Добавить новую станцию">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Button>
                <Button fx:id="btnSaveStations" layoutX="328.0" layoutY="264.0" mnemonicParsing="false"
                        text="Сохранить все станции">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Button>
                <Button fx:id="btnGetAllStations" layoutX="44.0" layoutY="264.0" mnemonicParsing="false"
                        text="Выгрузить все файлы">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Button>
                <Button fx:id="btnBack" layoutX="253.0" layoutY="330.0" mnemonicParsing="false" text="Вернуться">
                    <font>
                        <Font size="14.0"/>
                    </font>
                </Button>
            </children>
        </AnchorPane>
    </children>

</AnchorPane>
